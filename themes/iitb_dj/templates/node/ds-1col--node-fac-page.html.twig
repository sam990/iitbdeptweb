{#
/**
 * @file
 * Display Suite 1 column template.
 *
 * Available variables:
 * - ds_content_wrapper: wrapper around content
 * - attributes: content region attributes
 * - ds_content: content region
 */
#}
	<header class="sabbi-page-header page-header-lg event-page-header" {% if faculty_header_style %} style = "{{ faculty_header_style }}" {% endif %}>
		<div class="page-header-content conternt-center">
			<div class="header-title-block">
		  </div>
		</div>
  </header>
<{{ ds_content_wrapper }}{{ attributes.addClass('ds-1col', 'clearfix') }}>

  {{ title_suffix.contextual_links }}

	<div class="container page-academix site-padding-single">
		<div class="row">
			<div class="col-sm-3 col-md-3 col-sm-offset-1 pull-up-240">
			 {{ ds_content.field_faculty_photo }}
				<div class="sp-blank-20"></div>
				<div class="addr_future_memb">
				{% if ds_content.field_room_no or ds_content.field_building_new %}
					<h4 class="entry-title">Location</h4>
					<div class="address-entry">
						<p class="__adtext">{{ ds_content.field_room_no.0 }}, {{ ds_content.field_building_new.0 }}</p>
					</div>
				{% endif %}
				{% if ds_content.field_phone_no_new or ds_content.field_email_new %}
					<footer class="contact-info">
						<h4 class="entry-title">Contact Information</h4>
						<p class="__ci_num inline"><span>{{ ds_content.field_phone_no_new }}</span></p>
						 <p class="__ci_num inline"><span>{{ ds_content.field_mobile_number_new	}}</span></p>
						<p class="__ci_num inline"> <span>{{ ds_content.field_email_new }}</span></p>
				  </footer>
				{% endif %}
				</div>
				<div class="mt-20">
					{{ ds_content.field_google_scholar_id }}
					{{ ds_content.field_scopus_id }}
					{{ ds_content.field_orcid }}
				</div>
				<div class="mt-20">
				{{ ds_content.field_detailed_information_page }}	
				</div>	

			</div>

			<div class="col-sm-8">
				<article class="profile-glimps">
					<h2 class="entry-title title-foc-md">{{ ds_content.field_full_name }}</h2>
					 <p class="text-foc-md"></p>
					 <p class="text-foc-md">{{ ds_content.field_employment_category }}</p>
					 <p class="text-foc-md">{{ ds_content.field_faculty_designation.0 }}</p>
				</article>
						<ul class="nav nav-pills profile-nav">
							<li class="active"><a data-toggle="pill" href="#personal"><i class="fa fa-user" aria-hidden="true"></i> Personal Information</a></li>
						
							{% if ds_content.field_faculty_rd_proj or ds_content.field_publications_link %}		
								<li><a data-toggle="pill" href="#rnd"><i class="fa fa-tasks" aria-hidden="true"></i> R&D</a></li>
							{% endif %}

							{% if ds_content.field_courses_taught %}
								<li><a data-toggle="pill" href="#teaching"> <i class="fa fa-book" aria-hidden="true"></i> Teaching</a></li>
							{% endif %}
							
							{% if ds_content.field_research_area %}
								<li><a data-toggle="pill" href="#research-area"><i class="fa fa-check-square-o" aria-hidden="true"></i> Research Areas</a></li>
							{% endif %}
						</ul>
						<div class="tab-content">
							<div class="personal tab-pane fade in active stage-content-biog" id="personal">

						  <section class="gimps-container">
								<div class="row">
									<div class="col-md-8 col-sm-10">
										{{ ds_content.field_faculty_cv }}
										{{ ds_content.field_background }}

														  <!-- <ol class="ol-timeline">
